<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="true">
  <local-check-config name="Metafacture Fix Checks" location="/metafacture-fix/config/checkstyle/checkstyle.xml" type="project" description="Checkstyle checks for metafacture-fix">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="Metafacture Fix Checks" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
  <filter name="DerivedFiles" enabled="true"/>
</fileset-config>
